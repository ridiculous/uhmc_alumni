	font-style: normal;
	font-weight: normal;
	color: #000;
	text-decoration: none;
}
.lowerLinks:visited {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	font-style: normal;
	font-weight: normal;
	color: #000;
	text-decoration: none;
}
.lowerLinks:hover {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	font-style: normal;
	font-weight: normal;
	color: #0099FF;
	text-decoration: underline;
}

.home_lowerLinks_title {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	font-style: normal;
	font-weight: bold;
	color: #FD7E01;
	text-decoration: underline;
}























