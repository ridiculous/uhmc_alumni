#other_program, #other_degree, #other_school {
    display: none;
    margin-bottom: 10px;
    border: 1px solid grey;
    padding: 5px;
    background: whitesmoke
}

#out {
    margin: 5px 10px 15px 15px;

}

#outer {
    width: 650px;
    height: 420px;
    margin: auto;
    margin-top: 75px;
}

pre {
    background-color: #eee;
    padding: 10px;
    font-size: 11px;
}

#pw_helper {
}

#confirm_pw_helper {
}

.too-short {
    color: red
}

.good-enough {
    color: green
}

.topper-title {
    position: relative;
    left: 300px;
    top: 5px;
    font-size: 50px;
    font-family: 'BRUSH SCRIPT MT', Geneva, Georgia;
    letter-spacing: .2em;
    color: #006699;
}

#top-container {
    padding: 5px;
    background-color: white;
    border-top-right-radius: 5px 5px;
    border-top-left-radius: 5px 5px;
    border-bottom-right-radius: 5px 5px;
    border-bottom-left-radius: 5px 5px;
}

.to-top {
    text-align: center;
    color: #006699;
    font-size: 16px
}

.reset {
    clear: both;
}

#sign-in {
    font-size: 18px;
    color: #003163;
    background-color: #D8D8D8;
    padding: 5px
}

.single-alumn-header {
    font-size: 17px;
    color: #B0B0B0;
}

.single-alumn-header a, .single-alumn-header a:visited {
    font-size: 17px;
    color: #069;
}

.single-alumn-header a:hover {
    font-size: 17px;
    color: #E39B00;
}

.shiftcontainer {
    position: relative;
    margin-top: 10px;
    margin-right: auto;
    margin-left: auto;
    width: 1160px;
    height: 100%;
}

.shadow {
    background-color: #AEAEAE;
}

.shadow .innerdiv {
    background-color: white;
    border: 1px solid gray;
    position: relative;
    left: -4px;
    top: -4px;
    padding: 10px;
}

.shadow, .shadow .innerdiv {
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
}

.sub-container {
    width: 530px;
    height: auto;
    border: none;
}

.sub-header {
    color: #B0B0B0;
    font-size: 24px;
    width: 45%;
    padding-bottom: 3px;
}

.sub-header .alumni-name {
    color: #104D71;
}

.sub-header a {
    color: #B0B0B0;
    font-size: 17px;
}

.sub-header a:hover {
    color: #003163;
    font-size: 17px;
}

.txt-required {
    font-style: italic;
    font-size: 11px
}

#user-search-div {
    padding-left: 10px;
    padding-bottom: 5px;
    display: none
}

#user-search-button-span {
    padding: 10px;
    padding-left: 0px;
    font-size: 16px;
}

.employment-date-checkbox {
    margin-left: -100px !important;
    margin-right: -100px !important;
}

a.edit,
a.delete,
a.details,
a.summary {
    padding: 3px 14px;
}

a.edit {
    background: url(/images/icons/187-pencil.png) bottom left no-repeat;
}

a.details {
    background: url(/images/icons/06-magnify.png) bottom left no-repeat;
}

a.delete {
    background: url(/images/icons/146-gavel.png) bottom left no-repeat;
}

a.summary {
    background: url(/images/icons/33-cabinet.png) bottom left no-repeat;
}

#close_form {
    display: none;
}

.fl {
    float: left;
}

.fr {
    float: right;
}