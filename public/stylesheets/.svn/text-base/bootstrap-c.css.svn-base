form .field{margin-bottom:10px;zoom:1;}
form label{font-size:10pt;color:#555;font-weight:bold;}
form input[type=text],form input[type=password]{padding:6px;font-size:11pt;}
form select{padding:2px;font-size:12pt;}
form .submit{padding:10px;border-top:1px solid #CCC;background-color:#F4F4F4;margin-top:10px;}
form.side label{float:left;width:130px;padding-top:6px;text-align:right;}
form.side .input{margin-left:150px;display:block;}
table.list{font-size:10pt;background-color:#FFF;border:0px solid #0881BE;padding:1px;width:100%;-moz-border-radius:8px;border-radius:8px;}table.list tr.odd{background-color:#FDFDFD;}
table.list tr:hover{background-color:#ffffff;cursor:pointer;}
table.list td{border-bottom:1px dashed #EEE;border-left:0px dashed #7EBFDF;border-collapse:collapse;padding:4px 8px;}
table.list th{background-color:#0169cf;color:white;text-shadow:1px 1px #0A4184;padding:4px 8px;border:0px solid #0881BE;border-right:1px solid #FFF;font-weight:bold;}
table.list th a{text-decoration:none;color:white;}
table.list .null{-moz-border-radius:3px;border-radius:3px;background-color:#DDD;color:#888;font-style:italic;padding:1px 6px;text-transform:uppercase;text-shadow:1px 1px #FFF;font-size:8pt;}
